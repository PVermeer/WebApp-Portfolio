{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js",
          "/*.woff2"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ],
        "urls": [
          "https://fonts.googleapis.com/**",
          "https://fonts.gstatic.com/**",
          "/content/image**",
          "/content/page**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api's",
      "urls": [ ],
      "version": "",
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "7d",
        "timeout": "2s",
        "strategy": "performance"
      }
    }
  ]
}
